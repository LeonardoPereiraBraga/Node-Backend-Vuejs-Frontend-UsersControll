{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  created() {\n    const req = {\n      //Precisa do token para essa requisicao da nossa API\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem('token')\n      }\n    };\n    axios.get(\"http://localhost:8686/user/\" + this.$route.params.id, req).then(res => {\n      this.name = res.data.name;\n      this.email = res.data.email;\n    }).catch(error => {\n      console.log(error.response);\n      this.$router.push({\n        name: 'Users'\n      }); //redirect\n    });\n  },\n  data() {\n    return {\n      name: '',\n      email: '',\n      error: undefined\n    };\n  },\n  methods: {\n    register() {\n      axios.post(\"http://localhost:8686/user\", {\n        name: this.name,\n        email: this.email\n      }).then(res => {\n        this.$router.push({\n          name: 'home'\n        });\n        console.log(res);\n      }).catch(error => {\n        const msgErro = error.response.data.err;\n        this.error = msgErro;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","created","req","headers","Authorization","localStorage","getItem","get","$route","params","id","then","res","name","data","email","catch","error","console","log","response","$router","push","undefined","methods","register","post","msgErro","err"],"sources":["C:\\Users\\braga\\OneDrive\\Documentos\\Vue Front + Node Back\\usersvuejs\\src\\views\\EditView.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h2>Edi√ßao de Usuario</h2>\r\n        <hr>\r\n        <div class=\"columns is-mobile is-centered\">\r\n            <div class=\"column is-half\">\r\n                <div v-if=\"error != undefined\" class=\"alert alert-danger mb-2\">\r\n                    <p> {{ error }}</p>\r\n                </div>\r\n\r\n                <p>Nome</p>\r\n                <input class=\"input\" type=\"text\" placeholder=\"Nome do usuario\" v-model=\"name\"